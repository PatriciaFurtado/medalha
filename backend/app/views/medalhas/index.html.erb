
<h1 class="topoCrud">Medalhas</h1>

<table class="table">
  <thead>
    <tr>
      <th>Titulo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medalhas.each do |medalha| %>
      <tr>
        <td><%= medalha.titulo %></td>
        <!--<td><%= link_to 'Show', medalha %></td>
        <td><%= link_to 'Edit', edit_medalha_path(medalha) %></td>
        <td><%= link_to 'Destroy', medalha, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
      </tr>
    <% end %>
  </tbody>
</table>
<p id="notice"><%= notice %></p>

<br>

